import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
            int n=scn.nextInt();
        // write ur code here
        int p=n;
        for(int i=1;i<=n;i++){
            for(int j=1;j<=p;j++){
                System.out.print("*	");
            }
            p--;
            System.out.println();
        }
    }
}

